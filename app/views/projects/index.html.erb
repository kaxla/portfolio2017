<h1>Index of Projects</h1>
<% @projects.each do |p| %>
  <%= link_to "#{p.title}", project_path(p) %>
  <%= p.description %>
  <%= link_to "Edit", edit_project_path(p) %>
  <%= link_to "Delete", project_path(p), method: :delete, data: {confirm: "No, but really?"} %>
<% end %>

<%= link_to 'Create New Project', new_project_path %>
